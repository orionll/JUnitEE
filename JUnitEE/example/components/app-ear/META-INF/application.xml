<application>
	<display-name>JUnitEE Example</display-name>
	
	<module>
		<ejb>junitee-example-ejb.jar</ejb>
	</module>
	
	<module>
		<web>
			<web-uri>junitee-example-web.war</web-uri>
			<context-root>/example</context-root>
		</web>
	</module>

	<module>
		<web>
			<web-uri>junitee-example-test.war</web-uri>
			<context-root>/exampletest</context-root>
		</web>
	</module>
	
	<security-role>
		<role-name>users</role-name>
	</security-role>
	
</application>
