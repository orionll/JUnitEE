<ejb-jar>

	<enterprise-beans>
		<session>
			<description>
				The Einstein session bean is capable of adding two numbers
				together (well, it tries).
			</description>
			
			<ejb-name>EinsteinEJB</ejb-name>
			
			<home>org.junitee.ejb.einstein.EinsteinHome</home>
			<remote>org.junitee.ejb.einstein.Einstein</remote>
			<ejb-class>org.junitee.ejb.einstein.EinsteinEJB</ejb-class>
			
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>

	<assembly-descriptor>
	</assembly-descriptor>
	
</ejb-jar>
